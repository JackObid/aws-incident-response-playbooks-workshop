# .aws/credentials example file to be used by AWS CLI
# to assume roles using the source principal (default is TeamRole)
# replace 111122223333 with the actual AWS account number used
[profile SecurityAnalystRole]
role_arn = arn:aws:iam::111122223333:role/SecurityAnalystRole
source_profile = Environment
[profile SecurityDeployRole]
role_arn = arn:aws:iam::111122223333:role/SecurityDeployRole
source_profile = Environment
[profile SecurityBreakGlassRole]
role_arn = arn:aws:iam::111122223333:role/SecurityBreakGlassRole
source_profile = Environment
[profile SecurityAdminRole]
role_arn = arn:aws:iam::111122223333:role/SecurityAdminRole
source_profile = Environment